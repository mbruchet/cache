<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net471</TargetFrameworks>
    <RuntimeIdentifier>win7-x64</RuntimeIdentifier>
    <Platforms>AnyCPU;x64</Platforms>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Description>Cache core to inject cache project</Description>
    <Copyright>Mega 1980-2018</Copyright>
    <PackageLicenseUrl>http://www.mega.com</PackageLicenseUrl>
    <PackageProjectUrl>http://www.mega.com/hopex</PackageProjectUrl>
    <PackageIconUrl>http://www.mega.com/sites/giga/themes/giga/assets/giga-logo-no-baseline.png</PackageIconUrl>
    <RepositoryUrl>https://megainternational.pkgs.visualstudio.com/_packaging/HopexVNextSF/nuget/v3/index.json</RepositoryUrl>
    <RepositoryType>VSTS</RepositoryType>
    <PackageTags>Hopex, core</PackageTags>
    <PackageReleaseNotes>Core</PackageReleaseNotes>
    <Authors>Mega</Authors>
    <Company>Mega</Company>
    <Product>Hopex</Product>
    <VersionPrefix>1.0</VersionPrefix>
